generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Products {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  code String @unique
  status String
  imported_t DateTime
  url String
  creator String
  created_t String
  last_modified_t String
  product_name String
  quantity String
  brands String
  categories String
  labels String
  cities String
  purchase_places String
  stores String
  ingredients_text String
  traces String
  serving_size String
  serving_quantity Float
  nutriscore_score Int
  nutriscore_grade String
  main_category String
  image_url String

  @@map("products")
}

model FilesInfo {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  start_line Int
  last_update DateTime

  @@map("files_info")
}
